-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•

local ExecutorFeatures = {
    filtergc = type(filtergc) == "function",
    getgc = type(getgc) == "function",
    getsenv = type(getsenv) == "function",
    islclosure = type(islclosure) == "function",
    hookfunction = type(hookfunction) == "function",
    getfunctionhash = type(getfunctionhash) == "function",
    setclipboard = type(setclipboard) == "function",
    getupvalues = type(getupvalues) == "function",
    setupvalue = type(setupvalue) == "function"
}

local function Notify(title, text, duration)
    pcall(function()
        StarterGui:SetCore("SendNotification", {
            Title = title,
            Text = text,
            Duration = duration or 3
        })
    end)
end

if not ExecutorFeatures.getgc and not ExecutorFeatures.filtergc then
    warn("âŒ GetGC Explorer: Delta Executor no compatible detectado")
    return
end

