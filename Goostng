local function UpdateCanvas(frame, layout)
    frame.CanvasSize = UDim2.new(0, 0, 0, layout.AbsoluteContentSize.Y + 20)
end

ListLayout:GetPropertyChangedSignal("AbsoluteContentSize"):Connect(function() 
    UpdateCanvas(ListFrame, ListLayout) 
end)

InfoLayout:GetPropertyChangedSignal("AbsoluteContentSize"):Connect(function() 
    UpdateCanvas(InfoFrame, InfoLayout) 
end)

local function ClearList()
    for _, child in ipairs(ListFrame:GetChildren()) do
        if child:IsA("TextButton") then child:Destroy() end
    end
end

local function ClearInfo()
    for _, child in ipairs(InfoFrame:GetChildren()) do
        if child:IsA("TextLabel") or child:IsA("TextButton") then child:Destroy() end    end
end

local function AddInfo(text, isBold, colorOverride)
    local lbl = Instance.new("TextLabel")
    lbl.BackgroundTransparency = 1
    lbl.Size = UDim2.new(1, -10, 0, isBold and 35 or 28)
    lbl.Text = "  " .. text
    lbl.TextColor3 = colorOverride or (isBold and Color3.fromRGB(200, 200, 255) or Color3.new(1, 1, 1))
    lbl.TextScaled = true
    lbl.TextXAlignment = Enum.TextXAlignment.Left
    lbl.Font = isBold and Enum.Font.GothamBold or Enum.Font.Code
    lbl.TextWrapped = true
    lbl.Parent = InfoFrame
    return lbl
end

-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
-- ANÃLISIS DE FUNCIONES
-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•

local function GetFunctionDetails(func)
    local details = {
        info = {},
        scriptName = "Desconocido",
        isLua = false,
        hash = "N/A",
        upvalues = {},
        constants = {},
        fenvKeys = {}
    }
    
    -- InformaciÃ³n bÃ¡sica
    local info = debug.getinfo(func, "nSlu")
    if info then details.info = info end
    
    -- Entorno
    if ExecutorFeatures.getsenv then
        local env = getsenv(func)
        if env then
            details.scriptName = env.script and (env.script.Name or env.script.ClassName) or "Desconocido"
            
            for k, _ in pairs(env) do 
                table.insert(details.fenvKeys, tostring(k)) 
            end
            table.sort(details.fenvKeys)
        end
    end
    
    -- Tipo de funciÃ³n    if ExecutorFeatures.islclosure then
        details.isLua = islclosure(func)
    end
    
    -- Hash (para funciones C)
    if not details.isLua and ExecutorFeatures.getfunctionhash then
        details.hash = getfunctionhash(func) or "N/A"
    end
    
    -- Upvalues
    local i = 1
    while true do
        local name, val = debug.getupvalue(func, i)
        if not name then break end
        
        local valStr
        if type(val) == "table" then
            valStr = "Tabla(" .. tostring(#val) .. ")"
        elseif type(val) == "function" then
            valStr = "FunciÃ³n"
        elseif type(val) == "string" then
            valStr = '"' .. string.sub(val, 1, 30) .. (string.len(val) > 30 and "..." or "") .. '"'
        else
            valStr = tostring(val)
        end
        
        details.upvalues[name] = valStr
        i += 1
    end
    
    -- Constantes
    i = 1
    while true do
        local c = debug.getconstant(func, i)
        if c == nil then break end
        
        local cStr
        if type(c) == "string" then
            cStr = '"' .. string.sub(c, 1, 30) .. (string.len(c) > 30 and "..." or "") .. '"'
        else
            cStr = tostring(c)
        end
        
        table.insert(details.constants, cStr)
        i += 1
    end
    
    return details
end
-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
-- MOSTRAR DETALLES
-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•

local function ShowObjectDetails(obj, pos)
    ClearInfo()
    local objType = type(obj)
    AddInfo("[" .. pos .. "/" .. #FilteredObjects .. "] Tipo: " .. objType, true)
    
    if objType == "function" then
        local details = GetFunctionDetails(obj)
        
        AddInfo("ğŸ“„ Script: " .. details.scriptName)
        AddInfo("ğŸ“› Nombre: " .. (details.info.name or "AnÃ³nima"))
        AddInfo("ğŸ“ LÃ­neas: " .. details.info.linedefined .. " - " .. details.info.lastlinedefined)
        AddInfo("ğŸ”— Tipo: " .. (details.isLua and "Lua Closure" or "C Function" .. (details.hash and " (Hash: " .. details.hash .. ")" or "")))
        
        -- Upvalues
        if next(details.upvalues) then
            AddInfo("ğŸ“ˆ Upvalues (" .. #details.upvalues .. "):", true)
            for name, val in pairs(details.upvalues) do
                AddInfo("  " .. name .. " â†’ " .. val)
            end
        end
        
        -- Constantes
        if #details.constants > 0 then
            AddInfo("âš™ï¸ Constants (" .. #details.constants .. "):", true)
            local shown = 0
            for _, c in ipairs(details.constants) do
                if shown < 20 then
                    AddInfo("  " .. c)
                    shown += 1
                end
            end
            if shown < #details.constants then
                AddInfo("  ... (" .. (#details.constants - shown) .. " mÃ¡s)")
            end
        end
        
        -- Fenv Keys
        if #details.fenvKeys > 0 then
            AddInfo("ğŸŒ Fenv Keys (" .. #details.fenvKeys .. "):", true)
            local shown = 0
            for _, k in ipairs(details.fenvKeys) do
                if shown < 20 then
                    AddInfo("  " .. k)
                    shown += 1
                end
            end            if shown < #details.fenvKeys then
                AddInfo("  ... (" .. (#details.fenvKeys - shown) .. " mÃ¡s)")
            end
        end
        
        -- Botones interactivos
        local btnSize = UDim2.new(0.32, 0, 0, 45)
        local yPos = 1
        
        -- BotÃ³n Hook
        if ExecutorFeatures.hookfunction then
            local HookBtn = Instance.new("TextButton")
            HookBtn.Size = btnSize
            HookBtn.Position = UDim2.new(0, 5, yPos, -200)
            HookBtn.BackgroundColor3 = Config.Colors.ButtonWarning
            HookBtn.Text = "ğŸ”— Hook"
            HookBtn.TextScaled = true
            HookBtn.Font = Enum.Font.GothamBold
            HookBtn.Parent = InfoFrame
            
            local HookCorner = Instance.new("UICorner")
            HookCorner.CornerRadius = UDim.new(0, 6)
            HookCorner.Parent = HookBtn
            
            HookBtn.MouseButton1Click:Connect(function()
                local old = hookfunction(obj, function(...)
                    print("ğŸª HOOK [" .. pos .. "]:", ...)
                    return old(...)
                end)
                Notify("GetGC", "FunciÃ³n [" .. pos .. "] enganchada exitosamente!", 3)
            end)
            yPos = yPos + 0.34
        end
        
        -- BotÃ³n Ejecutar
        local ExecBtn = Instance.new("TextButton")
        ExecBtn.Size = btnSize
        ExecBtn.Position = UDim2.new(0.34, 0, 1, -200)
        ExecBtn.BackgroundColor3 = Config.Colors.ButtonPrimary
        ExecBtn.Text = "â–¶ï¸ Ejecutar"
        ExecBtn.TextScaled = true
        ExecBtn.Font = Enum.Font.GothamBold
        ExecBtn.Parent = InfoFrame
        
        local ExecCorner = Instance.new("UICorner")
        ExecCorner.CornerRadius = UDim.new(0, 6)
        ExecCorner.Parent = ExecBtn
        
        ExecBtn.MouseButton1Click:Connect(function()
            local ok, res = pcall(obj)            Notify("GetGC", ok and "âœ… Ejecutado correctamente" or "âŒ Error: " .. tostring(res), 3)
        end)
        
        -- BotÃ³n Copiar
        if ExecutorFeatures.setclipboard then
            local CopyBtn = Instance.new("TextButton")
            CopyBtn.Size = btnSize
            CopyBtn.Position = UDim2.new(0.68, 0, 1, -200)
            CopyBtn.BackgroundColor3 = Config.Colors.ButtonInfo
            CopyBtn.Text = "ğŸ“‹ Copiar"
            CopyBtn.TextScaled = true
            CopyBtn.Font = Enum.Font.GothamBold
            CopyBtn.Parent = InfoFrame
            
            local CopyCorner = Instance.new("UICorner")
            CopyCorner.CornerRadius = UDim.new(0, 6)
            CopyCorner.Parent = CopyBtn
            
            CopyBtn.MouseButton1Click:Connect(function()
                local details = GetFunctionDetails(obj)
                local infoText = string.format(
                    "GetGC [%d]\nTipo: %s\nScript: %s\nNombre: %s\nLÃ­neas: %d-%d\nUpvalues: %d\nConstants: %d",
                    pos, objType, details.scriptName, details.info.name or "AnÃ³nima",
                    details.info.linedefined, details.info.lastlinedefined,
                    #details.upvalues, #details.constants
                )
                setclipboard(infoText)
                Notify("GetGC", "âœ… InformaciÃ³n copiada al portapapeles", 2)
            end)
        end
        
    elseif objType == "table" then
        local keys = {}
        for k in pairs(obj) do table.insert(keys, tostring(k)) end
        table.sort(keys)
        
        AddInfo("ğŸ“Š Claves (" .. #keys .. "):")
        local shown = 0
        for _, k in ipairs(keys) do
            if shown < 30 then
                AddInfo("  " .. k)
                shown += 1
            end
        end
        if shown < #keys then
            AddInfo("  ... (" .. (#keys - shown) .. " mÃ¡s)")
        end
        
    elseif objType == "string" then
        local preview = string.len(obj) > 300 and string.sub(obj, 1, 300) .. "..." or obj        AddInfo("ğŸ“ Longitud: " .. string.len(obj))
        AddInfo("ğŸ” Preview:", true)
        AddInfo(preview)
        
    else
        AddInfo("ğŸ“‹ Detalles:", true)
        AddInfo(tostring(obj))
    end
end

-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
-- ACTUALIZAR LISTA
-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•

local function UpdateObjectList()
    local currentTime = tick()
    if currentTime - LastUpdateTime < Config.UpdateDebounce then return end
    LastUpdateTime = currentTime
    
    local searchLower = string.lower(SearchBox.Text)
    ClearList()
    local shown = 0
    
    for pos, obj in ipairs(FilteredObjects) do
        local objType = type(obj)
        local detail = ""
        
        if objType == "function" then
            local info = debug.getinfo(obj)
            if ExecutorFeatures.getsenv then
                local env = getsenv(obj)
                if env and env.script then
                    detail = env.script.Name:match("([^/]+)$") or (info.name or "anon")
                else
                    detail = info.name or "anon"
                end
            else
                detail = info.name or "anon"
            end
            
            if ExecutorFeatures.islclosure and islclosure(obj) then 
                detail = "Lua: " .. detail 
            end
            
        elseif objType == "table" then 
            detail = #obj .. " keys"
        elseif objType == "string" then 
            detail = string.len(obj) .. " chars"
        end
        
        local searchStr = string.lower(objType .. " " .. detail)
        if searchLower == "" or string.find(searchStr, searchLower, 1, true) then
            shown += 1
            if shown > Config.MaxItems then break end
            
            local btn = Instance.new("TextButton")
            btn.Name = tostring(pos)
            btn.Size = UDim2.new(1, -12, 0, 50)
            btn.BackgroundColor3 = Color3.fromRGB(55, 55, 70)
            btn.BorderSizePixel = 0
            btn.Text = string.format("[%d/%d] %s%s", pos, #FilteredObjects, objType, detail ~= "" and ": " .. string.sub(detail, 1, 60) or "")
            btn.TextColor3 = Color3.new(1, 1, 1)
            btn.TextScaled = true
            btn.Font = Enum.Font.Gotham
            btn.TextWrapped = true
            btn.Parent = ListFrame
            
            local btnCorner = Instance.new("UICorner")
            btnCorner.CornerRadius = UDim.new(0, 6)
            btnCorner.Parent = btn
            
            btn.MouseButton1Click:Connect(function()
                SelectedPosition = pos
                SelectedObject = obj
                ShowObjectDetails(obj, pos)
            end)
            
            btn.MouseEnter:Connect(function() 
                btn.BackgroundColor3 = Color3.fromRGB(75, 75, 100) 
            end)
            
            btn.MouseLeave:Connect(function() 
                btn.BackgroundColor3 = Color3.fromRGB(55, 55, 70) 
            end)
        end
    end
    
    ClearInfo()
    AddInfo(string.format("Mostrando %d/%d objetos", shown, #FilteredObjects), true)
end

-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
-- REFRESCAR GC
-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•

local function RefreshGCExplorer()
    local startTime = tick()
    ClearList()
    ClearInfo()
    AddInfo("ğŸ”„ Refrescando GC... (usando " .. (ExecutorFeatures.filtergc and "filtergc" or "getgc") .. ")", true)
    
    local filterFunc = GetFilterFunction(CurrentTab)
    
    if ExecutorFeatures.filtergc and filterFunc then
        FilteredObjects = filtergc(filterFunc)
    elseif ExecutorFeatures.getgc then
        FilteredObjects = getgc()
        
        -- Aplicar filtro manual si es necesario
        if filterFunc then
            local temp = {}
            for _, obj in ipairs(FilteredObjects) do
                if filterFunc(obj) then
                    table.insert(temp, obj)
                end
            end
            FilteredObjects = temp
        end
    else
        Notify("GetGC", "âŒ Error: No se encontrÃ³ getgc ni filtergc", 5)
        return
    end
    
    local elapsed = tick() - startTime
    ClearInfo()
    AddInfo(string.format("âœ… %d objetos encontrados (%.2fms)", #FilteredObjects, elapsed * 1000), true, Color3.fromRGB(100, 255, 100))
    UpdateObjectList()
end

-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
-- CONEXIONES DE EVENTOS
-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•

RefreshBtn.MouseButton1Click:Connect(RefreshGCExplorer)
CloseBtn.MouseButton1Click:Connect(function() 
    ScreenGui:Destroy() 
    print("GetGC Explorer cerrado.")
end)

-- BÃºsqueda con debounce
SearchBox.Changed:Connect(function(prop)
    if prop == "Text" then
        local currentTime = tick()
        if currentTime - LastSearchTime > Config.SearchDebounce then
            LastSearchTime = currentTime
            UpdateObjectList()
        end
    end
end)

-- Arrastrar ventana
TopBar.InputBegan:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 or 
       input.UserInputType == Enum.UserInputType.Touch then
        dragging = true
        dragStart = input.Position
        startPos = MainFrame.Position
        
        input.Changed:Connect(function()
            if input.UserInputState == Enum.UserInputState.End then
                dragging = false
            end
        end)
    end
end)

UserInputService.InputChanged:Connect(function(input)
    if dragging and (input.UserInputType == Enum.UserInputType.MouseMovement or 
                     input.UserInputType == Enum.UserInputType.Touch) then
        UpdateDrag(input)
    end
end)

UserInputService.InputEnded:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 or 
       input.UserInputType == Enum.UserInputType.Touch then
        dragging = false
    end
end)

-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
-- INICIALIZACIÃ“N
-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•

-- Activar primer tab
if #TabButtons > 0 then
    TabButtons[1].BackgroundColor3 = Config.Colors.TabActive
end

-- Refrescar inicial
RefreshGCExplorer()

-- Mensaje de carga
print("ğŸš€ GetGC Explorer v3.0 cargado exitosamente!")
print("   âœ… Executor Features:", table.concat(Object.keys(ExecutorFeatures), ", "))
print("   ğŸ“Š Objetos totales:", #FilteredObjects)
print("   ğŸ’¡ Usa la GUI para explorar, buscar y manipular objetos del GC")
Notify("GetGC Explorer", "âœ… Cargado exitosamente! " .. #FilteredObjects .. " objetos disponibles.", 5)
